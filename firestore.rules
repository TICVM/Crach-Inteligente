
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    
    function isSignedIn() {
      return request.auth != null;
    }

    match /alunos/{alunoId} {
      allow read, write: if isSignedIn();
    }

    match /modelosCracha/{modeloId} {
      allow read, write: if isSignedIn();
    }
    
    match /{path=**} {
      allow read, write: if false;
    }
  }
}
